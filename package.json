{
  "name": "adba",
  "version": "1.0.0-dev",
  "description": "Any DataBase to API",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts",
      "source": "./src/index.ts"
    },
    "./ts/*": "./src/*",
    "./esm/*": "./dist/esm/*",
    "./cjs/*": "./dist/cjs/*"
  },
  "directories": {
    "ts": "src",
    "esm": "dist/esm",
    "cjs": "dist/cjs"
  },
  "scripts": {
    "build:cjs": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "rm -rf dist/* && npm run build:cjs && npm run build:esm && typedoc",
    "doc": "typedoc",
    "test": "jest",
    "test:e2e": "jest --config ./jest.config.e2e.ts --forceExit"
  },
  "author": "joneldiablo",
  "license": "ISC",
  "dependencies": {
    "change-case-all": "^2.1.0",
    "dbl-utils": "^1.0.2",
    "express": "^4.21.1",
    "flat": "^5",
    "json2csv": "^6.0.0-alpha.2",
    "knex": "^3.1.0",
    "moment": "^2.30.1",
    "objection": "^3.1.5",
    "uuid": "^11.0.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/flat": "^5.0.5",
    "@types/jest": "^29.5.14",
    "@types/json2csv": "^5.0.7",
    "@types/lodash": "^4.17.12",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.7.9",
    "@types/uuid": "^10.0.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.11",
    "typescript": "^5.6.3"
  }
}
